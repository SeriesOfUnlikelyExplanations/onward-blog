{
  "name": "onward_blog",
  "version": "0.0.0",
  "author": "Tom Woodard",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git@github.com:SeriesOfUnlikelyExplanations/onward-blog.git"
  },
  "scripts": {
    "new": "hexo new",
    "hexo-deploy": "hexo deploy",
    "cdk-deploy": "cdk deploy --require-approval never --outputs-file output.json",
    "s3-deploy": "aws s3 sync public s3://`jq -r '.OnwardBlogStack.bucketName' output.json`",
    "invalidate": "aws cloudfront create-invalidation --distribution-id `jq -r '.OnwardBlogStack.distID' output.json` --paths '/*'",
    "deploy": "npm run hexo-deploy && npm run cdk-deploy && npm run s3-deploy && npm run invalidate",
    "serve": "hexo generate && hexo server --draft",
    "test": "echo suceeded",
    "publish": "hexo publish",
    "preinstall": "npx npm-force-resolutions"
  },
  "hexo": {
    "version": "5.4.0"
  },
  "dependencies": {
    "@aws-cdk/assert": "^1.87.0",
    "@aws-cdk/aws-cloudfront": "^1.87.0",
    "@aws-cdk/aws-route53-targets": "^1.87.0",
    "@aws-cdk/aws-s3": "^1.87.0",
    "@aws-cdk/aws-s3-deployment": "^1.87.0",
    "@aws-cdk/core": "^1.87.0",
    "@aws-cdk/yaml-cfn": "^1.87.0",
    "@types/node": "^10.17.27",
    "aws-cdk": "^1.87.0",
    "aws-sdk": "^2.841.0",
    "cdk-assets": "^1.87.0",
    "fs-extra": "^9.1.0",
    "hexo": "^5.4.0",
    "hexo-filter-responsive-images": "^1.7.0",
    "hexo-generator-archive": "^1.0.0",
    "hexo-generator-category": "^1.0.0",
    "hexo-generator-feed": "^3.0.0",
    "hexo-generator-index": "^2.0.0",
    "hexo-generator-tag": "^1.0.0",
    "hexo-image-tools": "git://github.com/SeriesOfUnlikelyExplanations/hexo-filter-responsive-images.git",
    "hexo-renderer-ejs": "^1.0.0",
    "hexo-renderer-marked": "^4.0.0",
    "hexo-renderer-stylus": "^2.0.0",
    "hexo-server": "^2.0.0",
    "highlight.js": "^10.6.0",
    "live-server": "^1.2.1",
    "npm-force-resolutions": "^0.0.3",
    "promptly": "^3.2.0",
    "source-map-support": "^0.5.16",
    "ts-node": "^9.0.0",
    "typescript": "^3.9.8"
  },
  "resolutions": {
    "axios": "^0.21.1",
    "marked": "^1.2.9"
  }
}
